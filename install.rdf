<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
  
    <em:id>{1A2D0EC4-75F5-4c91-89C4-3656F6E44B68}</em:id>
    <em:name>Image Zoom</em:name>
    <em:version>0.4.7</em:version>
    <em:description>Adds zoom functionality for images</em:description>
    <em:creator>Original by: Jason Adams Modded by: Dr. M</em:creator>
    <em:homepageURL>http://imagezoom.yellowgorilla.net/</em:homepageURL>
    <em:optionsURL>chrome://net.yellowgorilla.imagezoom/content/options.xul</em:optionsURL>   
    <em:iconURL>chrome://net.yellowgorilla.imagezoom/skin/imagezoom.png</em:iconURL>   
    <em:aboutURL>chrome://net.yellowgorilla.imagezoom/content/about.xul</em:aboutURL>

    <em:file>
      <Description about="urn:mozilla:extension:file:imagezoom.jar">
        <em:package>content/</em:package>
        <em:skin>skin/classic/imagezoom/</em:skin>
  	<em:locale>locale/en-US/</em:locale>
      </Description>
    </em:file>

    <!-- Firefox -->    
    <em:targetApplication>
     <Description>
       <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
       <em:minVersion>3.0</em:minVersion>
       <em:maxVersion>20.0.*</em:maxVersion>
     </Description>
    </em:targetApplication>    

    <!-- Thunderbird -->    
